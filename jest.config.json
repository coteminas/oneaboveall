{
  "testRegex": "spec.ts$",
  "moduleFileExtensions": ["js", "ts"],
  "coverageDirectory": "./coverage",
  "coverageReporters": ["text", "lcov", "html"],
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "<rootDir>/src/entity",
    "<rootDir>/src/types",
    "<rootDir>/src/inputs",
    "<rootDir>/src/migration",
    "<rootDir>/src/entities.ts",
    "<rootDir>/src/config.ts",
    "<rootDir>/src/resolvers/index.ts",
    "<rootDir>/src/util/query.ts"
  ],
  "collectCoverageFrom": [
    "src/**/*.ts*"
  ]
}
