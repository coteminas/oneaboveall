{
   "name": "oneaboveall",
   "version": "1.0.0",
   "main": "index.js",
   "license": "MIT",
   "scripts": {
      "build": "babel src -d dist --extensions .ts",
      "serve": "node dist/index.js",
      "start": "dotenv -e .env -- nodemon",
      "orm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
      "lint": "eslint src --ext .ts",
      "jest": "NODE_ENV=test node --expose-gc node_modules/.bin/jest --logHeapUsage",
      "migrate": "yarn orm migration:run",
      "gen:migration": "yarn orm migration:generate",
      "gen:entity": "plop",
      "gen:resolver": "plop"
   },
   "devDependencies": {
      "@babel/cli": "^7.10.5",
      "@babel/core": "^7.11.1",
      "@babel/plugin-proposal-class-properties": "^7.10.4",
      "@babel/plugin-proposal-decorators": "^7.10.5",
      "@babel/preset-env": "^7.11.0",
      "@babel/preset-typescript": "^7.10.4",
      "@types/jest": "^26.0.10",
      "@types/node": "^8.0.29",
      "@typescript-eslint/eslint-plugin": "^3.9.1",
      "@typescript-eslint/parser": "^3.9.1",
      "babel-jest": "^26.3.0",
      "babel-plugin-module-resolver": "^4.0.0",
      "babel-plugin-relative-path-import": "^2.0.1",
      "babel-plugin-transform-typescript-metadata": "^0.3.0",
      "class-validator": "^0.12.2",
      "dotenv-cli": "^3.2.0",
      "eslint": "^7.7.0",
      "handlebars-helpers": "^0.10.0",
      "inquirer-recursive": "^0.0.6",
      "jest": "^26.4.0",
      "nodemon": "^2.0.4",
      "plop": "^2.7.4",
      "sqlite3": "^5.0.0",
      "tsconfig-paths": "^3.9.0",
      "typescript": "^3.9.7"
   },
   "dependencies": {
      "apollo-server": "^2.16.1",
      "graphql": "^15.3.0",
      "graphql-parse-resolve-info": "^4.7.0",
      "lodash.get": "^4.4.2",
      "mongodb": "^3.6.2",
      "mysql": "^2.14.1",
      "pg": "^8.3.0",
      "pg-hstore": "^2.3.3",
      "reflect-metadata": "^0.1.13",
      "ts-node": "^9.0.0",
      "type-graphql": "^1.0.0-rc.3",
      "typeorm": "0.2.25",
      "winston": "^3.3.3"
   }
}
